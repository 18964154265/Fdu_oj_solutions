#include<string.h>

int main()
{
	char name[10000];
	gets(name);
	int n = strlen(name) + 19;
	for (int i = 1; i <= n; i++)
		printf("*");
	printf("\n");
	for (int i = 1; i <= 6; i++)
		printf(" "); printf("Hello ");
	printf("%s!      \n", name);

		
	for (int i = 1; i <= n; i++)
		printf("*");
   return 0;

}

